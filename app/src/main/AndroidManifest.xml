<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helloworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloWorld">
        <receiver
            android:name=".myAlarmReceiver"
            android:enabled="true"
            android:exported="true"></receiver>
        <receiver
            android:name=".MyAirplaneReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.AIRPLANE_MODE" />
            </intent-filter>
        </receiver>

        <service
            android:name=".OlahragaMyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".ProgressService"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <activity
            android:name=".finish"
            android:parentActivityName=".pendaki"/>
        <activity
            android:name=".pendaki"
            android:exported="false" />
        <activity
            android:name=".crunch"
            android:exported="false" />
        <activity
            android:name=".Olahraga"
            android:exported="false" />
        <activity
            android:name=".makanbang"
            android:parentActivityName=".alarm"
            />
        <activity
            android:name=".login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WaterIntake"
            android:exported="false" />
        <activity
            android:name=".Inputbmi"
            android:exported="false" />
        <activity
            android:name=".BmiActivity"
            android:exported="false" />
        <activity
            android:name=".alarm"
            android:exported="false" />
        <activity
            android:name=".MainActivity2"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>